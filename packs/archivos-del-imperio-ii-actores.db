"img":"modules/wfrp4e-core/icons/talents/combat-reflexes.png","effects":[{"_id":"7i8RGItRAF5GLESP","flags":{"wfrp4e":{"effectTrigger":"getInitiativeFormula","effectApplication":"actor","script":"let initiativeSetting = game.settings.get(\"wfrp4e\", \"initiativeRule\")\n\nswitch (initiativeSetting) {\n  case \"default\":\n    args.initiative += \"+10\"\n    break;\n\n  case \"sl\":\n    args.initiative += \"+1\"\n    break;\n\n  case \"d10Init\":\n    args.initiative += \"+10\"\n    break;\n\n  case \"d10InitAgi\":\n    args.initiative += \"+1\"\n    break;\n}","preventDuplicateEffects":false}},"changes":[],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"modules/wfrp4e-core/icons/talents/combat-reflexes.png","label":"Reflejos de combate","tint":null,"transfer":true,"origin":null}],"folder":null,"sort":0,"flags":{},"_id":"jao7pteqkcnppmdg","system":{"description":{"type":"String","label":"Description","value":"<p>Reaccionas como el fogonazo de un relámpago. Suma 10 a tu Iniciativa por cada nivel en este Talento cuando determinas la Iniciativa de Combate.</p>"},"gmdescription":{"type":"String","label":"Description","value":""},"max":{"type":"String","label":"Max Advances","value":"i"},"advances":{"type":"Number","label":"Advances","value":1,"force":false},"career":{"value":"","type":"String","label":"Career"},"tests":{"type":"String","label":"Tests","value":""},"source":{"type":"String","label":"Source"}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"name":"Pelea callejera","type":"talent","img":"modules/wfrp4e-core/icons/talents/dirty-fighting.png","effects":[{"_id":"NznDJLYiqbwPzhZt","changes":[],"disabled":false,"duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"modules/wfrp4e-core/ic